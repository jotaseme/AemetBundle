{
    "name" : "jotaeme/aemet-bundle",
    "description" : "Bundle de symfony para conectar con Aemet",
    "authors" : [{
        "name" : "jotaememoreno",
        "email" : "jotaememoreno@gmail.com"
    }, {
        "name" : "AemetBundle",
        "homepage" : "https://github.com/jotaseme/AemetBundle.git"
    }
    ],
    "license" : "MIT",
    "type" : "library",
    "require" : {
        "php" : ">=7.1.3",
        "symfony/framework-bundle": ">=2.7",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "stof/doctrine-extensions-bundle" : "^1.3",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev" : {
        "phpunit/phpunit" : "5.5.*"
    },
    "autoload" : {
        "psr-4" : {
            "Jotaeme\\AemetBundle\\" : ""
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Jotaeme\\AemetBundle\\Tests\\" : "Test"
        }
    }
}